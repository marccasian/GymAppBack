<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Abonament" table="abonament">
    <indexes>
      <index name="abonament_user_idx" columns="Username"/>
      <index name="abonament_abonamentType_idx" columns="AbonamentTypeId"/>
    </indexes>
    <id name="abonamentid" type="integer" column="AbonamentId">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="startdate" type="datetime" column="StartDate" nullable="true"/>
    <field name="enddate" type="datetime" column="EndDate" nullable="true"/>
    <field name="price" type="float" column="Price" precision="10" scale="0" nullable="true"/>
    <field name="level" type="integer" column="Level" nullable="true">

      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name = "active" type="integer" column="Active" ></field>
    <many-to-one field="abonamenttypeid" target-entity="Abonamenttype" fetch="LAZY">
      <join-columns>
        <join-column name="AbonamentTypeId" referenced-column-name="AbonamentTypeId"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="username" target-entity="User" fetch="LAZY">
      <join-columns>
        <join-column name="Username" referenced-column-name="Username"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
